alias: "Cold Weather Alert"
description: "Checks if temperature will drop below 1°C in the next 7 days"
trigger:
  - platform: state
    entity_id: weather.forecast_home
  - platform: time_pattern
    hours: "/1"  # Check every hour
condition:
  - condition: template
    value_template: >
      {{ state_attr('weather.forecast_home', 'temperature') < 1 }}
action:
  - service: persistent_notification.create
    data:
      title: "Cold Weather Alert"
      message: "Temperature will drop below 1°C"
